#include <stdio.h>

int main() {
    // Read input
    int n;
    scanf("%d",&n);
    
    // Validate input to prevent infinite loops
    if (n<1){
        printf("Invalid input");
        return(0);
    }
    // Create nested loops to print the multiplication table
    for (int i=1;i<=n;i++){
        for(int j=1;j<=n;j++){
            printf("%2d ",i*j);
        }
        printf("\n");
    }
    return 0;
}
